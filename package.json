{
  "name": "fleepy.tv",
  "author": "Flleeppyy",
  "description": "My website that I work on.",
  "website": "https://fleepy.tv",
  "scripts": {
    "upgradeall": "yarn upgrade && cd frontend && yarn upgrade && cd ../backend && yarn upgrade",
    "installall": "yarn && cd frontend_fancy && yarn && cd ../backend && yarn",
    "buildall": "shx rm -rf frontend_fancy/src/js && yarn frontend_fancy:sass:build && yarn frontend_fancy:build:prod",
    "build": "yarn buildall",
    "all": "yarn installall && yarn buildall",
    "backend:start": "yarn --cwd backend start",
    "backend:dev": "yarn --cwd backend dev",
    "frontend_fancy:sass:watch": "yarn --cwd frontend_fancy sass:watch",
    "frontend_fancy:sass:build": "yarn --cwd frontend_fancy sass:build",
    "frontend_fancy:build:dev": "yarn --cwd frontend_fancy build:dev",
    "frontend_fancy:build:prod": "yarn --cwd frontend_fancy build:prod",
    "frontend_fancy:build:devwatch": "yarn --cwd frontend_fancy build:devwatch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flleeppyy/fleepy.tv.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flleeppyy/fleepy.tv/issues"
  },
  "homepage": "https://github.com/flleeppyy/fleepy.tv#readme",
  "dependencies": {
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "nodemon": "^2.0.15",
    "shx": "^0.3.3",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.0.3",
    "uniq": "^1.0.1"
  }
}
