  "scripts": {
    "start": "yarn sass:build && tsc && NODE_ENV=production PORT=8001 ts-node ./src",
    "dev": "yarn sass:build && nodemon --ext 'ts' --watch 'src' --exec 'tsc && NODE_ENV=development PORT=8080 ts-node ./src'",
    "frontend:sass:watch": "yarn --cwd frontend sass:watch",
    "frontend:sass:build": "yarn --cwd frontend sass:build"
  },
