  "dependencies": {
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.0.3",
    "uniq": "^1.0.1"
  }
