{
  "name": "fleepy.tv",
  "author": "Flleeppyy",
  "description": "My website that I work on.",
  "website": "https://fleepy.tv",
  "scripts": {
    "upgradeall": "pnpm upgrade && cd frontend_fancy && pnpm upgrade && cd ../backend && pnpm upgrade",
    "installall": "pnpm install && cd frontend_fancy && pnpm install && cd ../backend && pnpm install",
    "buildall": "shx rm -rf frontend_fancy/src/js && pnpm run frontend_fancy:sass:build && pnpm run frontend_fancy:build:prod",
    "build": "pnpm run buildall",
    "all": "pnpm run installall && pnpm run buildall",
    "backend:build": "cd backend && pnpm run build",
    "backend:start": "cd backend && pnpm run start",
    "backend:dev": "cd backend && pnpm run dev",
    "frontend_fancy:sass:watch": "cd frontend_fancy && pnpm run sass:watch",
    "frontend_fancy:sass:build": "cd frontend_fancy && pnpm run sass:build",
    "frontend_fancy:build:dev": "cd frontend_fancy && pnpm run build:dev",
    "frontend_fancy:build:prod": "cd frontend_fancy && pnpm run build:prod",
    "frontend_fancy:build:devwatch": "cd frontend_fancy && pnpm run build:devwatch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flleeppyy/fleepy.tv.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "nodemon": "^2.0.15",
    "shx": "^0.3.3",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.0.3",
    "uniq": "^1.0.1"
  }
}
