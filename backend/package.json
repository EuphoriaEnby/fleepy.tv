{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production PORT=8001 node --require tsconfig-paths/register ./dist",
    "dev": "nodemon --ext 'ts' --watch 'src' --exec 'NODE_ENV=development PORT=8080 ts-node --require tsconfig-paths/register ./src'",
    "build": "tsc --project ./tsconfig.json && cp -r ./src/errors ./dist/errors && cp ./src/index.ejs ./dist/index.ejs"
  },
  "dependencies": {
    "@types/ejs": "^3.1.0",
    "@types/ws": "^8.2.2",
    "axios": "^0.25.0",
    "ejs": "^3.1.6",
    "fastify": "^3.14.1",
    "fastify-cors": "^6.0.2",
    "fastify-static": "^4.4.1",
    "pino": "^7.6.5",
    "pino-pretty": "^7.5.1",
    "socket.io": "^4.0.1",
    "ws": "^8.4.2"
  }
}
